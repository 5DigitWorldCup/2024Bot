{
  "name": "2024bot",
  "version": "1.0.0",
  "author": "5DigitWorldCup",
  "description": "5WC 2024 Discord Bot",
  "license": "ISC",
  "homepage": "https://github.com/5DigitWorldCup/2024Bot#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/5DigitWorldCup/2024Bot.git"
  },
  "bugs": {
    "url": "https://github.com/5DigitWorldCup/2024Bot/issues"
  },
  "main": "./dist/src/index.js",
  "exports": "./dist/src/index.js",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "format": "prettier --write ./src/",
    "lint": "eslint --cache --fix ./src/",
    "start": "cross-env TS_NODE_BASEURL=./dist/src/ node -r tsconfig-paths/register .",
    "dev": "ts-node --transpile-only -r tsconfig-paths/register src/index.ts",
    "dev:nm": "nodemon --config nodemon.json src/index.ts",
    "deploy": "cross-env TS_NODE_BASEURL=./dist/src/ node -r tsconfig-paths/register dist/src/discord/deploy-commands.js",
    "clear": "cross-env TS_NODE_BASEURL=./dist/src/ node -r tsconfig-paths/register dist/src/discord/clear-commands.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "discord.js": "^14.13.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.11.0",
    "winston-transport": "^4.6.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write ./src/"
    ]
  }
}
